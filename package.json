{
  "name": "my-angular",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ng serve --no-hmr",
    "build": "ng build",
    "watch": "ng build --watch",
    "prepare": "husky",
    "postinstall": "bun run lint:write",
    "eslint:check": "eslint .",
    "eslint:write": "eslint . --fix",
    "prettier:check": "prettier -c --log-level=warn .",
    "prettier:write": "prettier -lw --log-level=warn .",
    "lint:check": "bun run eslint:check & bun run prettier:check",
    "lint:write": "bun run eslint:write && bun run prettier:write",
    "npm:check": "bunx npm-check-updates"
  },
  "dependencies": {
    "@angular/animations": "20.2.4",
    "@angular/cdk": "20.2.2",
    "@angular/common": "20.2.4",
    "@angular/compiler": "20.2.4",
    "@angular/core": "20.2.4",
    "@angular/forms": "20.2.4",
    "@angular/platform-browser": "20.2.4",
    "@angular/platform-browser-dynamic": "20.2.4",
    "@angular/router": "20.2.4",
    "@ngrx/operators": "20.0.1",
    "@ngrx/signals": "20.0.1",
    "@primeng/themes": "20.1.1",
    "primeicons": "7.0.0",
    "primeng": "20.1.1",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular/build": "20.2.2",
    "@angular/cli": "20.2.2",
    "@angular/compiler-cli": "20.2.4",
    "@eslint/js": "9.34.0",
    "@tailwindcss/postcss": "4.1.12",
    "@types/node": ">=22.0.0 <23.0.0",
    "angular-eslint": "20.2.0",
    "eslint": "9.34.0",
    "husky": "9.1.7",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "tailwindcss": "4.1.12",
    "typescript": ">=5.9.0 <5.10.0",
    "typescript-eslint": "8.42.0"
  }
}
